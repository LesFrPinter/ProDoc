<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Views\UserControls\**" />
    <EmbeddedResource Remove="Views\UserControls\**" />
    <None Remove="Views\UserControls\**" />
    <Page Remove="Views\UserControls\**" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="Images\PDS-logo-lrg.png" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Images\PDS-logo-lrg.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="EntityFramework" Version="6.4.4" />
    <PackageReference Include="Extended.Wpf.Toolkit" Version="4.5.0" />
    <PackageReference Include="Microsoft.NETCore.Targets.NETFramework" Version="4.6.0" />
    <PackageReference Include="telerik.ui.for.wpf.netcore.xaml" Version="2023.1.117" />
  </ItemGroup>

</Project>
